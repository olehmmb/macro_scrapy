{
    "name": "Scraping",
    "build":{
        "dockerfile": "Dockerfile",
        "context": "../",
        "args": { "HTTP_PROXY": "${localEnv:HTTP_PROXY}",
                  "HTTPS_PROXY": "${localEnv:HTTPS_PROXY}" 
                }
    },
    "containerEnv": { "HTTP_PROXY": "${localEnv:HTTP_PROXY}",
                   "HTTPS_PROXY": "${localEnv:HTTPS_PROXY}" 
  },
    "customizations": {
        "vscode": {
            "extensions": [
                "eamodio.gitlens",
                "grapecity.gc-excelviewer",
                "mechatroner.rainbow-csv",
                "ms-python.python",
                "ms-python.flake8"
            ]
        }
    }  
}

